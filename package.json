{
  "name": "react-reusable",
  "version": "5.0.0",
  "description": "react-reusable React component",
  "main": "lib/index.js",
  "module": "es/index.js",
  "files": [
    "css",
    "es",
    "lib",
    "umd"
  ],
  "scripts": {
    "prepublish": "npm run build",
    "build": "nwb build-react-component",
    "clean": "nwb clean-module && npm clean-demo",
    "start": "nwb serve-react-demo",
    "test": "nwb test-react",
    "test:coverage": "nwb test-react --coverage",
    "test:watch": "nwb test-react --server",
    "format": "prettier --trailing-comma es5 --single-quote --write 'src/**/*.js' 'src/*/!(node_modules)/**/*.js' 'demo/src/**/*.js'"
  },
  "dependencies": {
    "history": "^4.7.2",
    "lodash": "^4.17.4",
    "md5": "^2.2.1"
  },
  "peerDependencies": {
    "lodash": "4.x",
    "react": "15.x"
  },
  "devDependencies": {
    "gh-pages": "^1.1.0",
    "nwb": "0.20.x",
    "prettier": "^1.9.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "author": "",
  "homepage": "",
  "license": "MIT",
  "repository": "",
  "keywords": [
    "react-component"
  ]
}
